  "version": "0.2.0",
  "configurations": [
    {
      "name": "02-test.html",
      "type": "chrome",
      "request": "launch",
      "file": "${workspaceFolder}/packages/vue/examples/02-test.html",
    },
    {
      "type": "node",
      "name": "vscode-jest-tests",
      "request": "launch",
      "args": [
        // "Map.spec.ts",
        "componentEmits.spec.ts",
        "--runInBand"
      ],
      "cwd": "${workspaceFolder}",
      // "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "program": "${workspaceFolder}/node_modules/jest/bin/jest",
      "outputCapture": "std"
    },
  ]
}
